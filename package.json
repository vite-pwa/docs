    "eslint": "^8.54.0",
    "https-localhost": "^4.7.1",
    "typescript": "^5.4.5",
    "unocss": "^0.58.0",
    "unplugin-vue-components": "^0.25.2",
    "vite-plugin-pwa": "^0.20.0",
    "vitepress": "^1.1.4",
    "workbox-window": "^7.1.0"
