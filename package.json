{
  "name": "vite-pwa-docs",
  "type": "module",
  "version": "0.20.5",
  "packageManager": "pnpm@9.9.0",
  "description": "Zero-config PWA for Vite Documentation",
  "author": "antfu <anthonyfu117@hotmail.com>",
