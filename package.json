{
  "name": "vite-pwa-docs",
  "type": "module",
  "version": "1.1.0",
  "packageManager": "pnpm@10.18.2",
  "description": "Zero-config PWA for Vite Documentation",
  "author": "antfu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/vite-pwa/docs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vite-pwa/docs.git"
  },
  "bugs": "https://github.com/vite-pwa/docs/issues",
  "keywords": [
    "react",
    "pwa",
    "vue",
    "vitepress",
    "preact",
    "svelte",
    "sveltekit",
    "workbox",
    "solidjs",
    "vite",
    "vite-plugin",
    "astro",
    "astro-integration",
    "iles",
    "nuxt",
    "nuxt module"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "vitepress dev",
    "build": "vitepress build",
    "preview": "nr build && vitepress serve",
    "https": "nr build && serve .vitepress/dist",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "generate-pwa-icons": "pwa-assets-generator"
  },
  "dependencies": {
    "@vueuse/core": "^13.9.0",
    "@vueuse/shared": "^13.9.0",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@antfu/ni": "^26.1.0",
    "@types/node": "^22.9.3",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vite-pwa/vitepress": "^1.0.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vueuse/core": "^13.9.0",
    "eslint": "^9.37.0",
    "https-localhost": "^4.7.1",
    "oxc-minify": "^0.95.0",
    "typescript": "~5.9.3",
    "unocss": "^66.5.4",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.10",
    "vite-plugin-pwa": "^1.1.0",
    "vitepress": "^2.0.0-alpha.12",
    "vitepress-plugin-group-icons": "^1.6.4",
    "workbox-window": "^7.3.0"
  },
  "pnpm": {
    "overrides": {
      "oxc-minify": "^0.95.0",
      "vite": "npm:rolldown-vite@latest",
      "workbox-build": "^7.3.0",
      "workbox-window": "^7.3.0"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
}
